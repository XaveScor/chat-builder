{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": ["last 1 Chrome versions"]
                }
            }
        ],
        "@babel/preset-react",
        "@babel/preset-flow",
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {"loose": true}],
        "@babel/plugin-proposal-optional-chaining",
    ]
}
